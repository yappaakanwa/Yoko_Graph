            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".appendActivity"
            android:label="@string/title_activity_append" >
        </activity>
        <activity
            android:name=".infActivity"
            android:label="@string/title_activity_inf" >
        </activity>
        <activity
            android:name=".propertyActivity"
            android:label="@string/title_activity_property" >
        </activity>
        <activity
            android:name=".AwardActivity"
            android:label="@string/title_activity_award" >
        </activity>
    </application>

</manifest>
